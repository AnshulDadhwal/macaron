#include "prelude.dl"

Policy("check-dependencies", component_id, "Check the dependencies of pkg:private_domain.com/apache/maven") :-
    transitive_dependency(component_id, dep_0),
    is_component(dep_0, "pkg:maven/com.google.guava/guava@30.1-jre?type=jar"),transitive_dependency(component_id, dep_1),
    is_component(dep_1, "pkg:maven/org.mockito/mockito-core@3.2.0?type=jar").

apply_policy_to("check-dependencies", component_id) :-
    is_component(component_id, "pkg:private_domain.com/apache/maven").
